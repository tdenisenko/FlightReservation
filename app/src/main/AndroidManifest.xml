<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tdenisenko.flightreservation" >
    <!-- Allow to connect with internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".library.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".library.Cabin" >
            <intent-filter>
                <action android:name="com.example.tdenisenko.flightreservation.cabin" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".library.calender" >
            <intent-filter>
                <action android:name="com.example.tdenisenko.flightreservation.calender" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".library.personPicker" >
            <intent-filter>
                <action android:name="com.example.tdenisenko.flightreservation.personPicker" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".library.searchMy" >
            <intent-filter>
                <action android:name="com.example.tdenisenko.flightreservation.search" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".library.login"
            android:label="@string/Login" >
        </activity>
        <activity
            android:name=".library.SearchHtmlParser"
            android:label="@string/SearchHtmlParser" >
        </activity>
        <activity
            android:name=".library.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".library.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".library.DashboardActivity"
            android:label="@string/title_activity_dashboard" >
        </activity>


        <activity
            android:name="com.example.tdenisenko.flightreservation.login.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.tdenisenko.flightreservation.login.SignUPActivity"/>


    </application>

</manifest>
